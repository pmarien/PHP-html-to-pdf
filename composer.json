  "type": "library",
  "require": {
    "php": ">=8.1",
    "psr/http-message": "^1.0|^2.0",
    "psr/http-factory": "^1.0",
    "psr/http-client": "^1.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PMA\\HtmlToPdf\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "dev@philipp-marien.de"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "kriswallsmith/buzz": "^1.0",
    "nyholm/psr7": "^1.0"
  }
}
